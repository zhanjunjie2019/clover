{
    "swagger": "2.0",
    "info": {
        "description": "clover-auth接口文档",
        "title": "clover-auth-api",
        "contact": {},
        "version": "v1.0.0"
    },
    "basePath": "/auth",
    "paths": {
        "/tenant-create": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "初始化租户",
                "parameters": [
                    {
                        "description": "初始化租户",
                        "name": "data",
