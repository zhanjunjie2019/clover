            "properties": {
                "tenantID": {
                    "description": "租户ID，不传默认则随机生成",
                    "type": "string"
                },
                "tenantName": {
                    "description": "租户名",
                    "type": "string"
                }
            }
        },
        "vo.TenantCreateRspVO": {
            "type": "object",
            "properties": {
                "secretKey": {
                    "description": "租户密钥",
                    "type": "string"
                },
                "tenantID": {
                    "description": "租户ID",
                    "type": "string"
                }
            }
        }
    }
